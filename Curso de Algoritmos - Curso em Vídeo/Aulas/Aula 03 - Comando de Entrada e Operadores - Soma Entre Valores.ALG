algoritmo "Soma Entre Dois Valores"
var
   N1, N2, S:Inteiro
inicio
      //Recebe a informação do usuario
      Escreva("Digite o primeiro número: ")
      Leia(N1)
      Escreva("Digite o segundo número: ")
      Leia(N2)
      
      //Faz a operação soma
      S <- N1 + N2
      Escreva("A soma entre",N1," e",N2," é igual a",S)
      
fimalgoritmo