algoritmo "Olá, Mundo!"
var
   msg: Caractere
inicio
      //Escreve a mensagem no console
      Escreval("Olá, Mundo!")
      
      //Atribuiu  a mensagem a variavel
      msg <- "Estou programando"
      
      //Escreve a mensagem atribuida a variavel
      Escreval(msg)
fimalgoritmo